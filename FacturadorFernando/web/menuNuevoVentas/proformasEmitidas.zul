                                            <listcell>
                                                <decimalbox format="###.##" 
                                                            readonly="true"  
                                                            value="@bind(each.cabSubTotal)" width="100%" />
                                            </listcell>
                                            <listcell>
                                                <decimalbox format="###.##" 
                                                            readonly="true"  
                                                            value="@bind(each.cabTotal)" width="100%" />
                                            </listcell>                                            
                                        </listitem>
                                    </template>
                                </listbox>
